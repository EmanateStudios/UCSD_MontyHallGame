{
  "name": "monty-hall-game",
  "version": "1.0.0",
  "description": "A game to test how people learn when provided a game of probabilities with additional unknown factors left open to discovery.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "restart": "nodemon server.js",
    "dev": "parcel public/pages/game.html",
    "build": "parcel build public/pages/game.html -d ./build",
    "dev-cache": "parcel build public/pages/game.html --cache-dir cache/",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EmanateStudios/UCSD_MontyHallGame.git"
  },
